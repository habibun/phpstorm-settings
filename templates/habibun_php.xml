<templateSet group="habibun_php">
  <template name="lt_vd" value="echo '&lt;pre&gt;';var_dump($END$);echo '&lt;/pre&gt;';" description="pretty var_dump" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="lt-pu-dump" value="dd($this-&gt;client-&gt;getResponse()-&gt;getContent());" description="phpunit show response" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="lt-object" value="$this-&gt;getObject($CLASS_NAME$, $res['$ID$'])" description="temporary usages" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ID" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>