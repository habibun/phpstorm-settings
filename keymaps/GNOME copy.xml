<keymap version="1" name="GNOME copy" parent="Default for GNOME">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="shift ctrl u" />
    <keyboard-shortcut first-keystroke="alt u" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
</keymap>